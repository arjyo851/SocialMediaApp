<%- include('partials/header'); -%>
    <div class="container py-md-5 container--narrow" style="border: 1px solid black;">
        <div id="profile">
            <button class="font-awes-profile"><i class="fas fa-user-circle fa-3x"></i></button>
            <p id="pro-name">
                <%=(typeof title !="undefined" ? title : "" )%>
            </p>
        </div>

        <div class="d-flex profile-but" >
            <button class="prof-but-elem" id="Posts"><a href="/profile/<%=title%>">Posts: <%=posts.length%></a> </button>
            <button class="prof-but-elem" style="color: #0060b6;"><a href="/profile/<%=title%>/followers"
                    style="color: #0060b6;">Followers: 0</a></button>
            <button class="prof-but-elem" style="color: #0060b6;" id="third_button"><a href="/profile/<%=title%>/following"
                    style="color: #0060b6;">Following: 0</a></button>
        </div>
        <div >
            <% posts.forEach(function(post){ %>
                <!-- <div class="  content-list-body mt-4" > -->
                    <div class=" card-list-body" id="backwards">
    
                        <div class="card card-task">
    
                            <div class="card-body">
                                <div class="card-title">
                                    <a href="/post/<%= post._id%>">
                                        <h6 data-filter-by="text">
                                          <b><%= post.title %> </b>  on <%=post.datePosted.getDate()%>/<%=post.datePosted.getMonth()+1%>/<%=post.datePosted.getFullYear()%>
                                        </h6>
                                    </a>
                                </div>
                               
    
                            </div>
                        </div>
    
                    </div>
                <!-- </div> -->
                <% }) %>
        </div>
        
    </div>

        
    <%- include('partials/chat'); -%>
            


    

    <script src="/javascript/chat.js"></script>

    <%- include('partials/footer'); -%>